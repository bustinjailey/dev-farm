{
  "kilocode.autoApproveCommands": {
    "enabled": true,
    "allowedPaths": [
      "PLACEHOLDER_PATHS"
    ],
    "allowAllInWorkspace": true
  },
  "cline.autoApprove": {
    "enabled": true,
    "allowedDirectories": [
      "PLACEHOLDER_PATHS"
    ]
  },
  "cline.autoApproveCommands": [
    "ls",
    "cd",
    "pwd",
    "echo",
    "cat",
    "head",
    "tail",
    "less",
    "more",
    "grep",
    "find",
    "tree",
    "env",
    "printenv",
    "which",
    "whereis",
    "git status",
    "git log",
    "git diff",
    "git show",
    "git branch",
    "git remote",
    "touch",
    "mkdir",
    "cp",
    "mv",
    "rm",
    "rmdir",
    "chmod",
    "chown",
    "ln",
    "readlink",
    "ps",
    "top",
    "df",
    "du",
    "free",
    "uptime",
    "curl",
    "wget",
    "ssh",
    "scp",
    "rsync",
    "npm",
    "node",
    "python",
    "python3",
    "pip",
    "pip3"
  ],
  "chat.tools.terminal.autoApprove": {
    "/^ls\\b/": true,
    "/^cd\\b/": true,
    "/^pwd\\b/": true,
    "/^echo\\b/": true,
    "/^cat\\s+[^~\\/]/": true,
    "/^head\\b/": true,
    "/^tail\\b/": true,
    "/^less\\b/": true,
    "/^more\\b/": true,
    "/^grep\\b/": true,
    "/^find\\s+/": true,
    "/^tree\\b/": true,
    "/^env\\b/": true,
    "/^printenv\\b/": true,
    "/^which\\b/": true,
    "/^whereis\\b/": true,
    "/^git\\s+(status|log|diff|show|branch|remote)\\b/": true,
    "/^touch\\b/": true,
    "/^mkdir\\b/": true,
    "/^cp\\s+/": true,
    "/^mv\\s+/": true,
    "/^chmod\\b/": true,
    "/^chown\\b/": true,
    "/^ln\\b/": true,
    "/^readlink\\b/": true,
    "/^ps\\b/": true,
    "/^top\\b/": true,
    "/^df\\b/": true,
    "/^du\\b/": true,
    "/^free\\b/": true,
    "/^uptime\\b/": true,
    "/^npm\\s+(list|ls|view|search|info|outdated)\\b/": true,
    "/^node\\s+-v/": true,
    "/^python3?\\s+--version/": true,
    "/^pip3?\\s+(list|show|search)\\b/": true,
    "rm": false,
    "rmdir": false,
    "kill": false,
    "sudo": false,
    "curl": false,
    "wget": false,
    "ssh": false,
    "scp": false,
    "rsync": false
  }
}