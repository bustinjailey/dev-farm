{
  "workbench.colorTheme": "Default Dark Modern",
  "editor.fontSize": 16,
  "editor.fontFamily": "Consolas, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.shellIntegration.enabled": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.emptyWindow": false,
  "security.workspace.trust.trustedFolders": [
    "/home/coder/workspace",
    "/home/coder"
  ],
  "explorer.confirmDelete": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true,
    "scminput": true
  },
  "github.copilot.chat.welcomeMessage": "never",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.chat.model": "claude-sonnet-4.5",
  "chatgpt.provider": "github-copilot",
  "chatgpt.model": "claude-sonnet-4.5",
  "cline.apiProvider": "anthropic",
  "cline.anthropicModel": "claude-sonnet-4-20250514",
  "cline.apiKey": "",
  "kilocode.defaultModel": "claude-sonnet-4.5",
  "kilocode.apiProvider": "anthropic",
  "workbench.activityBar.location": "default",
  "workbench.panel.defaultLocation": "bottom",
  "workbench.editor.showTabs": "multiple",
  "workbench.sideBar.location": "left",
  "workbench.secondarySideBar.defaultVisibility": true,
  "window.zoomLevel": 1,
  "chat.editor.fontSize": 16,
  "chat.editor.lineHeight": 24,
  "workbench.layoutControl.enabled": false,
  "workbench.statusBar.visible": true,
  "remote.SSH.showLoginTerminal": true,
  "remote.SSH.connectTimeout": 60,
  "workbench.startupEditor": "none",
  "isTrusted": true,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "markdown.preview.scrollEditorWithPreview": true,
  "markdown.preview.scrollPreviewWithEditor": true,
  "notifications.silent": false,
  "workbench.statusBar.suppressInSecureWarning": true,
  "keyboard.dispatch": "keyCode",
  "window.titleBarStyle": "custom",
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "window.restoreWindows": "none",
  "window.restoreFullscreen": false,
  "workbench.editor.restoreViewState": false
}